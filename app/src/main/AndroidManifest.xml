<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="exportkit.xd">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/wiggle_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.buydog_edititem_activity"></activity>
        <activity android:name=".activity.buydog_additem_activity" />
        <activity android:name=".activity.reset_password_activity" />
        <activity android:name=".activity.editprofile_activity" />
        <activity
            android:name=".activity.home_activity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.buydog_detail_activity" />
        <activity android:name=".activity.service_detail_activity" />
        <activity android:name=".activity.buydogs_activity" />
        <activity android:name=".activity.changepassword_activity" />
        <activity android:name=".activity.profile_activity" />
        <activity android:name=".activity.register_activity" />
        <activity android:name=".activity.services_activity" />
    </application>

</manifest>